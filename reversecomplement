def rev_comp(seq):
    revcomp = ''
    for i in seq:
        i = i.upper()
        if i == 'A':
            revcomp +=('T')
        elif i == 'T':
            revcomp +=('A')
        elif i == 'G':
            revcomp +=('C')
        elif i == 'C':
            revcomp +=('G')
    return revcomp[::-1]

if __name__ == '__main__':
    print(rev_comp('atgcctga'))
